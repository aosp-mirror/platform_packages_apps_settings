    <color name="sfps_enrollment_fp_captured_color">#d2e3fc</color> <!-- Blue 100 -->
    <color name="sfps_enrollment_fp_error_color">#fad2cf</color> <!-- Red 100 -->
    <color name="sfps_enrollment_progress_bar_bg_color">#3C4043</color> <!-- Gray 800 -->
